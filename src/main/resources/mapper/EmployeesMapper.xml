<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org/DTO Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo.mapper.EmployeesMapper">
    <select id="getAllEmployees" resultType="com.example.demo.vo.EmployeesDTO">
        select * from employees;
    </select>

    <select id="getSelectEmployees" resultType="com.example.demo.vo.EmployeesDTO">
        select * from employees where employeeNumber = '${employeeNumber}';
    </select>

</mapper>
